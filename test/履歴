<?php
$arr = explode(" ", fgets(STDIN));
$n = intval($arr[0]);
$y = intval($arr[1]);

echo "N: $n 枚\n";
echo "Y: $y 円\n";

$resultY = 0;
// Y円を表す最小枚数を検証する
$tenThousandCount = intdiv($y,10000);
$resultY = $y - $tenThousandCount*10000;

$fiveThousandCount = intdiv($resultY,5000);
$resultY -= $fiveThousandCount*5000;

$thousandCount = intdiv($resultY,1000);
$resultY -= $fiveThousandCount*1000;

$sumCount = $tenThousandCount+$fiveThousandCount+$thousandCount;

echo "  万:$tenThousandCount 枚\n";
echo " 5千:$fiveThousandCount 枚\n";
echo "  千:$thousandCount 枚\n";
echo "合計:$sumCount 枚\n";


// 検証


$whileCnt=0;
while(true){
    if($sumCount > $n){
        $diffCount = $sumCount - $n;
        $changeCount1000_10000 = intdiv($diffCount,9);
        $diffCount = $diffCount%9;
        $changeCount1000_5000 = intdiv($diffCount,4);
        $changeCount5000_10000 = $diffCount%4;

        for($i=0;$i<$changeCount1000_10000;$i++){
            $tenThousandCount += 1;
            $thousandCount -= 10;
        }
        for($i=0;$i<$changeCount1000_5000;$i++){
            $fiveThousandCount += 1;
            $thousandCount -= 5;
        }
        for($i=0;$i<$changeCount5000_10000;$i++){
            $tenThousandCount += 1;
            $fiveThousandCount -= 2;
        }
        if($sumCount = $n){
            echo $tenThousandCount," ",$fiveThousandCount," ",$thousandCount;
            break;
        }else{
            echo "-1 -1 -1";
            break;
        }
    }
    if($sumCount == $n){
        echo $tenThousandCount," ",$fiveThousandCount," ",$thousandCount;
        break;
    }
    if($tenThousandCount>0){
        $tenThousandCount--;
        $fiveThousandCount += 2;
        $sumCount = $tenThousandCount+$fiveThousandCount+$thousandCount;
        echo "1万円札を5千円札に崩した。現在$sumCount 枚\n";
        echo $tenThousandCount," ",$fiveThousandCount," ",$thousandCount,"\n";
    }elseif($fiveThousandCount>0){
        $fiveThousandCount--;
        $thousandCount += 5;
        $sumCount = $tenThousandCount+$fiveThousandCount+$thousandCount;
        echo "5千円札を千円札に崩した。現在$sumCount 枚\n";
        echo $tenThousandCount," ",$fiveThousandCount," ",$thousandCount,"\n";
    }
    
    $whileCnt++;
    if($whileCnt>=10000){
        break;
    }
}








<?php
$s = rtrim(fgets(STDIN));
$flag_a = false;
$flag_b = false;
$flag_c = false;
for($i=0;$i<3;$i++){
    $str = substr($s,$i,1);
    if($str === "A"){
        $flag_a = true;
    }elseif($str === "B"){
        $flag_b = true;
    }elseif($str === "C"){
        $flag_c = true;
    }
}
if($flag_a && $flag_b && $flag_c){
    echo "Yes";
}else{
    echo "No";
}

?>



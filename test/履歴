<?php

$n = intval(fgets(STDIN));
$arr = array_fill(0,$n,null);

for($i=0;$i<$n;$i++){
    $arr[$i] = intval(fgets(STDIN));
}

/*
echo '$n:',$n,"\n";
for($i=0;$i<$n;$i++){
    echo '$dArr[',"$i",']',"$dArr[$i]\n";
}
*/

$ret = 0;
$stacked = 101;
$roopCnt = 0;
while(true){
    $del = 0;
    $max = 0;
    for($i=0;$i<count($arr);$i++){
        if($max < $arr[$i]){
            $max = $arr[$i];
            $del = $i;
        }
    }
    echo "残りのお餅で一番大きい:$max\n";
    
    // 残りのお餅を減らす
    array_splice($arr, $del, 1);
    
    if($stacked > $max){
        // $maxを積む
        $stacked = $max;
        echo "積んだお餅：$stacked,";
        // 積んだ数をインクリする
        $ret++;
        echo "積んだ段数：$ret,";
        echo "残りのお餅：";
        for($i=0;$i<count($arr);$i++){
            echo "$arr[$i],";
        }
        echo "\n";
    }else{
        echo "積まない";
    }
    $roopCnt++;
    if($roopCnt == $n){
        break;
    }
}

echo $ret;


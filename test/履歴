<?php
$n = intval(fgets(STDIN));
$arr = array_fill(0,$n,null);
for($i=0;$i<$n;$i++){
    $arr[$i] = explode(" ", fgets(STDIN));
}
$nowX = 0;
$nowY = 0;
$nowTime = 0;
foreach( $arr as $val ){
    $time = $val[0];
    $x = $val[1];
    $y = $val[2];
    $farTime = $time - $nowTime;
    $far = abs($nowX - $x)+abs($nowY - $y);
    if(
        $far == $farTime
        || (
                $farTime > $far
            &&  ($farTime - $far) % 2 == 0
        )
    ){
        $nowX = $x;
        $nowY = $y;
        $nowTime = $time;
    }else{
        echo "No";
        exit;
    }
}

echo "Yes";


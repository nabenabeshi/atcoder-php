<?php
$arr = explode(" ", fgets(STDIN));
$n = intval($arr[0]);
$y = intval($arr[1]);
$resultY = 0;
$tenThousandCount = intdiv($y,10000);
$resultY = $y - $tenThousandCount*10000;
$fiveThousandCount = intdiv($resultY,5000);
$resultY -= $fiveThousandCount*5000;
$thousandCount = intdiv($resultY,1000);
$resultY -= $fiveThousandCount*1000;
$sumCount = $tenThousandCount+$fiveThousandCount+$thousandCount;
if($sumCount == $n){
    echo $tenThousandCount," ",$fiveThousandCount," ",$thousandCount;
    exit;
}
while(true){
    if($fiveThousandCount>0){
        $fiveThousandCount--;
        $thousandCount += 5;
        $sumCount = $tenThousandCount+$fiveThousandCount+$thousandCount;
        echo "5千円札を千円札に崩した。現在$sumCount 枚\n";
        echo $tenThousandCount," ",$fiveThousandCount," ",$thousandCount,"\n";
    }elseif($tenThousandCount>0){
        $tenThousandCount--;
        $fiveThousandCount += 2;
        $sumCount = $tenThousandCount+$fiveThousandCount+$thousandCount;
        echo "1万円札を5千円札に崩した。現在$sumCount 枚\n";
        echo $tenThousandCount," ",$fiveThousandCount," ",$thousandCount,"\n";
    }
    if($sumCount > $n){
        $diffCount = $sumCount - $n;
        $changeCount1000_10000 = intdiv($diffCount,9);
        $diffCount = $diffCount%9;
        $changeCount1000_5000 = intdiv($diffCount,4);
        $changeCount5000_10000 = $diffCount%4;

        if($thousandCount >= 10*$changeCount1000_10000){
            $tenThousandCount += 1*$changeCount1000_10000;
            $thousandCount -= 10*$changeCount1000_10000;
        }
        
        if($thousandCount >= 5*$changeCount1000_5000){
            $fiveThousandCount += 1*$changeCount1000_5000;
            $thousandCount -= 5*$changeCount1000_5000;
        }
        
        if($fiveThousandCount >= 2*$changeCount5000_10000){
            $tenThousandCount += 1*$changeCount5000_10000;
            $fiveThousandCount -= 2*$changeCount5000_10000;
        }
        
        $sumCount = $tenThousandCount+$fiveThousandCount+$thousandCount;
        if($sumCount != $n){
            echo "-1 -1 -1";
            exit;
        }
    }
    if($sumCount == $n){
        echo $tenThousandCount," ",$fiveThousandCount," ",$thousandCount;
        exit;
    }
}
?>

$n = intval($arr[0]);
$y = intval($arr[1]);
$resultY = 0;
$tenThousandCount = intdiv($y,10000);
$resultY = $y - $tenThousandCount*10000;
$fiveThousandCount = intdiv($resultY,5000);
$resultY -= $fiveThousandCount*5000;
$thousandCount = intdiv($resultY,1000);
$resultY -= $fiveThousandCount*1000;
$sumCount = $tenThousandCount+$fiveThousandCount+$thousandCount;
if($sumCount == $n){
    echo $tenThousandCount," ",$fiveThousandCount," ",$thousandCount;
    exit;
}
while(true){
    if($tenThousandCount>0){
        $tenThousandCount--;
        $fiveThousandCount += 2;
        $sumCount = $tenThousandCount+$fiveThousandCount+$thousandCount;
        echo "1万円札を5千円札に崩した。現在$sumCount 枚\n";
        echo $tenThousandCount," ",$fiveThousandCount," ",$thousandCount,"\n";
    }elseif($fiveThousandCount>0){
        $fiveThousandCount--;
        $thousandCount += 5;
        $sumCount = $tenThousandCount+$fiveThousandCount+$thousandCount;
        echo "5千円札を千円札に崩した。現在$sumCount 枚\n";
        echo $tenThousandCount," ",$fiveThousandCount," ",$thousandCount,"\n";
    }
    if($sumCount > $n){
        $diffCount = $sumCount - $n;
        
        if($fiveThousandCount >= 2*$diffCount){
            $tenThousandCount += 1*$diffCount;
            $fiveThousandCount -= 2*$diffCount;
        }
        
        $sumCount = $tenThousandCount+$fiveThousandCount+$thousandCount;
        if($sumCount != $n){
            echo "-1 -1 -1";
            exit;
        }
    }
    if($sumCount == $n){
        echo $tenThousandCount," ",$fiveThousandCount," ",$thousandCount;
        exit;
    }elseif($sumCount < $n && $tenThousandCount ==0 && $fiveThousandCount == 0){
        echo "-1 -1 -1";
        exit;
    }
}

<?php
$s = rtrim(fgets(STDIN));
$continueFlag = true;
while( mb_strlen($s) > 0 ){
    $continueFlag = false;
    echo $s,"\n";
    echo mb_strlen($s),"\n";
    
    $str = substr($s,0,11);
    if( $str == "dreameraser" ){
        $s = substr($s,11);
        echo "残りの文字列：[",$s,"]";
        $continueFlag = true;
    }
    
    $str = substr($s,0,10);
    if( $str == "dreamerase" ){
        $s = substr($s,10);
        echo "残りの文字列：[",$s,"]";
        $continueFlag = true;
    }

    $str = substr($s,0,7);
    if( $str == "dreamer"){
        $s = substr($s,7);
        echo "残りの文字列：[",$s,"]";
        $continueFlag = true;
    }
    

    $str = substr($s,0,5);
    if( $str == "dream" || $str == "erase"){
        $s = substr($s,5);
        echo "残りの文字列：[",$s,"]";
        $continueFlag = true;
    }
    
    
    if(!$continueFlag){
        break;
    }

}
echo "終わった時： $s", mb_strlen($s), "\n";
if(mb_strlen($s) == 0){
    echo "YES";
}else{
    echo "NO";
}



        if($retX == $x){
            $retX = 0;
        }else{
            $retX = intval($arr[$i][0]);
        }

        if($retY == $y){
            $retY = 0;
        }else{
            $retY = intval($arr[$i][1]);
        }
    }
    
    echo $retX," ",$retY,"\n";
}


<?php
$n = intval(rtrim(fgets(STDIN)));
$s = rtrim(fgets(STDIN));
$changeCount = 0;

echo $n,"\n";
echo $s,"\n";

for($i=0;$i<$n*2-1;$i++){
    echo "i:", $i , "\n";
    $now = substr($s,$i,1);
    echo "現在値：",$now,"\n";
    for($j=$i+1;$j<$n*2;$j++){
        echo "j:",$j , "\n";
        $next = substr($s,$j,1);
        echo "次の値：",$next,"\n";
        if($now === $next){
            echo "おんなじなので変えないといけないよ\n";
            // 入れ替え処理
            for($k=$j+1;$k<$n*2;$k++){
                $change = substr($s,$k,1);
                echo "変更対象の値：",$change,"\n";
                if($now === $change){
                    echo "変えられないよ\n";
                    continue;
                }else{
                    echo "変えられるよ\n";
                    // 変更対象の添字
                    echo $k,"番目の文字",$change,"を",$j,"番目の文字",$next,"とかえるよ\n";
                    
                    for($l=0;$l<$k-$j;$l++){
                        $tmp = $s[$k-1-$l];
                        $s[$k-1-$l] = $s[$k-$l];
                        $s[$k-$l] = $tmp;
                        $changeCount++;
                        echo "変えたよ", $s, " ",$changeCount,"回\n";
                    }
                    
                    break;
                }
            }
            echo "kループ終わり $s \n";
            break;
        }else{
            echo "ちがうのでかえなくていいよ\n";
            break;
        }
    }
    echo "jループ終わり $s \n";
}
echo "iループ終わり $s \n";
echo $changeCount;
?>


